{
  "name": "noem/framework-module",
  "description": "description",
  "type": "library",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "license": "MIT",
  "authors": [
    {
      "name": "biont",
      "email": "arbelzapf@gmail.com"
    }
  ],
  "require": {
    "lium/event-dispatcher": "dev-master",
    "noem/container": "dev-main",
    "noem/state-machine-module": "dev-main"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "mockery/mockery": "^1.4",
    "squizlabs/php_codesniffer": "^3.6"
  },
  "extra": {
    "noem": {
      "factories": "factories.php",
      "extensions": "extensions.php"
    }
  },
  "autoload": {
    "psr-4": {
      "Noem\\Framework\\": "src/"
    },
    "files": [
      "inc/functions.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Noem\\Framework\\Tests\\": "tests/"
    }
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/Biont/event-dispatcher"
    }
  ]
}
